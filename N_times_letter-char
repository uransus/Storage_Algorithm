#include <iostream>
#include <time.h>		
#include <stdlib.h>
#include <conio.h>		
#include <string.h>		
#include <math.h>		
#include <iostream>		
#include <fstream>
#include <ctype.h>
using namespace std;


int main()
{	do
	{
	int i, j;
	int times = 2;
	char a[100];
	cin.getline(a, 100);
	int len = strlen(a);
	for (i = 0; i < len; i++)
	{
		for (j = 0; j < times; j++)
		{
			cout << a[i];
		}
	}

		cout << "\n";
	} while (_getch() != 27);	
	return 0;
}
//Input: Hello
//Output: HHeelllloo
